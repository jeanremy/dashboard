<!DOCTYPE html>
<html lang="fr">
<head>
	<meta charset="UTF-8">
	{% block title %}<title>Dashboard</title>{% endblock %}

  	{% block stylesheets %}
   	 	<link rel="stylesheet" href="{{ asset('bundles/flyddashboard/css/main.min.css') }}" type="text/css" media="all" />
  	{% endblock %}

</head>
<body>

	<!-- Aside -->
	{% include "FlydDashboardBundle::menu.html.twig" %}

	<!-- Main -->
	<div class="main">

		{% include "FlydDashboardBundle::flash.html.twig" %}
		<!-- Top Bar -->
		<header class="header">			
			<div class="breadcrumb">
				{% block top %}

				{% endblock %}
			</div>
		</header>

		<div class="content">

			<div class="container">
				{% block body %}

			  	{% endblock %}
			</div>

		</div>
		
	</div>
	

	{% block basejs %}
   	 	<script type="text/javascript" src="{{ asset('bundles/flyddashboard/js/vendor/jquery/dist/jquery.min.js') }}"></script>
   	 	<script type="text/javascript" src="{{ asset('bundles/flyddashboard/js/main.js') }}"></script>
  	{% endblock %}
  	{% block js %}
  	{% endblock %}
</body>
</html>
